package lesson3;
public class Exercise4 {
    public static void main(String[] args) {
        int time = 20;
        switch (time) {
            case 21:
            case 22:
            case 23:
            case 24:
            case 1:
            case 2:
            case 3:
            case 4:
            case 5:
                System.out.println("Good night");
                break;
            default:
                System.out.println("Good day");
        }
    }
}
